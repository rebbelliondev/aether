{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.22000.0",
            "compilerPath": "C:/MinGW/bin/g++.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x86",
            "compilerArgs": [
                "-march=x86-64",
                "-mabi=sysv",
                "-mno-80387",
                "-mno-mmx",
                "-mno-sse",
                "-mno-red-zone",
                "-Wno-write-strings",
                "-MMD",
                "-MP",
                "-Iinclude/",
                "-I."
            ]
        }
    ],
    "version": 4
}